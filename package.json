    "url": "https://bitcoinerlab.com"
  },
  "homepage": "https://bitcoinerlab.com",
  "license": "MIT",
  "scripts": {
    "devel": "concurrently \"npm run build:client\" \"npm run serve:dev\"",
    "build": "concurrently \"npm run build:client\" && \"npm run build:server\"",
    "serve:prod": "pm2 start dist/server.bundle.js",

    "build:client": "rollup -c rollup.config.client.js -w --bundleConfigAsCjs",
    "build:server": "rollup -c rollup.config.server.js -w --bundleConfigAsCjs",
    "serve:dev": "npx nodemon --watch dist/public/bundle.js --exec babel-node src/server.entry.js"
  },
  "babel": {
    "presets": ["@babel/preset-env", "@babel/preset-react"]
  },
  "devDependencies": {
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.5",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "concurrently": "^7.6.0",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "rollup": "^3.7.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^4.3.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5"
  }
}
